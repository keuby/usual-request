{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.json": "jsonc",
    "package.json": "json"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "prettier.enable": true
}
